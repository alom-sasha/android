<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
<item>
    <shape>
        <corners
            android:radius="200dp"
            />
        <solid
            android:color="#FF441B"
            />
        <padding
            android:left="0dp"
            android:top="0dp"
            android:right="0dp"
            android:bottom="0dp"
            />
        <size
            android:width="250dp"
            android:height="250dp"
            />
    </shape>
</item>
<item android:drawable="@drawable/ic_alert_white"
    android:gravity="center"
    android:width="150dp"
    android:height="150dp">

    <solid
        android:color="#FFFFFF"
        />

</item>
</layer-list>
